<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
      fx:controller="client.ClientController"
      xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
         <children>
            <HBox fx:id="loginPanel" prefHeight="49.0" prefWidth="640.0">
               <children>
                  <TextField />
                  <TextField />
                  <Button fx:id="signUp" mnemonicParsing="false" text="Зарегистрироваться" />
                  <Button fx:id="signIn" mnemonicParsing="false" text="Войти" />
               </children>
            </HBox>
            <AnchorPane layoutY="49.0" prefHeight="323.0" prefWidth="640.0">
               <children>
                  <Label layoutX="137.0" layoutY="6.0" text="Server" />
                  <Label layoutX="485.0" layoutY="6.0" text="Client" />
                  <TextField fx:id="tfLogin" layoutX="8.0" layoutY="32.0" prefHeight="25.0" prefWidth="240.0" />
                  <TextField fx:id="tfPass" layoutX="360.0" layoutY="32.0" prefHeight="25.0" prefWidth="240.0" />
                  <ListView fx:id="lvServer" layoutX="7.0" layoutY="62.0" prefHeight="257.0" prefWidth="270.0" />
                  <ListView fx:id="lvClient" layoutX="360.0" layoutY="62.0" prefHeight="257.0" prefWidth="270.0" />
                  <Button layoutX="252.0" layoutY="32.0" mnemonicParsing="false" text="^" />
                  <Button layoutX="605.0" layoutY="32.0" mnemonicParsing="false" text="^" />
                  <Button layoutX="304.0" layoutY="78.0" mnemonicParsing="false" text="&gt;&gt;" />
                  <Button layoutX="304.0" layoutY="115.0" mnemonicParsing="false" text="&lt;&lt;" />
               </children>
            </AnchorPane>
         </children>
    </AnchorPane>
  </children>
</VBox>
